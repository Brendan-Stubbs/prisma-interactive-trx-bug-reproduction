generator client {
  provider = "prisma-client-js"
  previewFeatures = ["interactiveTransactions"]

}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Organization {
  organizationId  String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  @@map(name: "organization")
}
